<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="459.0" prefWidth="490.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xal.extension.jelog.PostEntryController">
   <children>
      <VBox layoutX="4.0" layoutY="6.0" prefHeight="389.0" prefWidth="400.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <Label text="Make an entry in eLog:">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </VBox.margin></Label>
            <GridPane prefHeight="244.0" prefWidth="470.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="286.0" minWidth="10.0" prefWidth="198.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="382.0" minWidth="10.0" prefWidth="382.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="20.0" prefHeight="40.0" vgrow="SOMETIMES" />
              </rowConstraints>              
               <children>
                  <Label text="Author :" GridPane.rowIndex="1">                    
                  </Label>
                  <Label text="Type :" GridPane.rowIndex="2">                    
                  </Label>
                  <Label text="Subject:" GridPane.rowIndex="4" />
                  <Label text="Include image:" GridPane.rowIndex="5" />
                  <Label text="Text:" GridPane.rowIndex="6" />
                  <Label text="LogBook" />
                  <Label text="Category:" GridPane.rowIndex="3" />
                  <ComboBox fx:id="comboBoxLogBook" onAction="#comboBoxLogBookHandler" prefHeight="27.0" GridPane.columnIndex="1" />
                  <TextField fx:id="textAuthor" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="comboBoxType" prefHeight="27.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="comboBoxCategory" prefHeight="27.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="textSubject" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <RadioButton fx:id="rbimageYes" mnemonicParsing="false" selected="true" text="Yes" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <toggleGroup>
                        <ToggleGroup fx:id="groupImage" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="rbimageNo" mnemonicParsing="false" text="No" toggleGroup="$groupImage" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <GridPane.margin>
                        <Insets left="60.0" />
                     </GridPane.margin></RadioButton>
               </children>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </GridPane>
            <TextArea fx:id="textBody" prefHeight="185.0" prefWidth="470.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextArea>
            <ButtonBar prefHeight="61.0" prefWidth="475.0">
              <buttons>
                <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#handleButtonCancel" text="Cancel" />
                  <Button fx:id="buttonSubmit" mnemonicParsing="false" onAction="#handleButtonSubmit" text="Submit" />
              </buttons>
               <VBox.margin>
                  <Insets right="5.0" />
               </VBox.margin>               
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
